.count
  clear: left
  float: left
  margin-right: 5px
  text-align: right
  width: 40px

.year-range
  color: gray
  font-size: 0.9em
  font-style: italic

.expand
  float: none
  font-size: 1.2em
  overflow: hidden
  padding-left: 5px

.arrow
  float: right
  height: 100%
  text-align: center
  width: 30px

.arrow:hover
  background-color: #eee

.expand:hover
  background-color: #eee

.expand:hover *
  color: black

.col
  height: 25px
  line-height: 25px

.res-container
  margin-left: 50px
