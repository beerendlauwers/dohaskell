.tag-count
  clear: left
  float: left
  font-weight: bold
  text-align: center
  width: 30px

.tag-expand
  color: #069
  float: none
  font-size: 22px
  font-weight: bold
  overflow: hidden
  padding-left: 5px

.tag-arrow
  float: right
  height: 100%
  text-align: center
  width: 30px

.tag-arrow:hover
  background-color: #eee

.tag-expand:hover
  color: #000
  background-color: #eee

.tag-col
  height: 30px
  line-height: 30px

.res-container
  margin-left: 35px
